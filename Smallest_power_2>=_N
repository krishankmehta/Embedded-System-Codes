int main()
{
    int num;
    int count;
    cout<<"Input the number :"<<endl;
    cin>>num;
    if(n==0)
    {
        cout<<'1';
    }
    while(num!=0)
    {
        num=num>>1;
        count+=1;
    }
    for(int i=0; i<=count; ++i)
    {

        num=(1<<count);


    }
    cout<<num<<endl;
}
