//To compare whether two numbers are equal without comparision sign
//Author : Krishank S. Mehta
//Date   : 3/3/2017
//Time   : 10:38 PM


#include<iostream>

using namespace std;


bool equal(int num1,int num2)
{

    return (num1^num2);

}

int main()
{
    int num1,num2;
    cout<<"Enter the 2 numbers"<<endl;
    cin>>num1>>num2;
    int x = !equal(num1,num2);
    if(x==1)
    {
        cout<<"True"<<endl;
    }
    else
    {
        cout<<"False"<<endl;
    }

}
